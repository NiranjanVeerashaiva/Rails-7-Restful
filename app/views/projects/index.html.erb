<div>
  <h1>Projects</h1>
  <%= turbo_stream_from "projects" %>

  <a href="<%= new_project_path %>" class="block text-white bg-green-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-2 py-2.5 text-center w-[100px] m-5" data-turbo-stream="true" data-turbo-frame="modal_frame"> <span>ADD NEW</span></a>
  <turbo-frame id="modal_frame"></turbo-frame>

  <div id="projects-list" class="">
    <div class="ml-5">
      <% @projects.each do |project| %>
        <%= render partial: "projects/project", locals: { project: project} %>
      <% end %>
    </div>
  </div>
</div>
<div>
  <%= turbo_frame_tag "show_project" %>
</div>

